{
  "Elasticsearch": {
    "DefaultIndex": "products",
    "Url": "http://localhost:9200/"
  },
  "KafkaConsumer": {
    "ConsumerConfig": {
      "GroupId": "Search",
      "BootstrapServers": "localhost:9092",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false
    },
    "Topics": [
      "Stores"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}