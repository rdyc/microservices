    "Scheme": "http",
    "Host": "localhost",
    "Port": 8500,
    "Type": "Consul"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "https",
      "UpstreamPathTemplate": "/lookup/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
